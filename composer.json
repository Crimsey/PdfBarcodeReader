{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.13",
        "nelmio/api-doc-bundle": "^4.3",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "5.3.*",
        "symfony/console": "*",
        "symfony/css-selector": "5.3.*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "*",
        "symfony/property-info": "5.3.*",
        "symfony/runtime": "*",
        "symfony/yaml": "*",
      "ext-http": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "conflict": {
            "symfony/symfony": "*"
        },
        "extra": {
            "symfony": {
                "allow-contrib": false,
                "require": "5.0.*"
            }
        },
        "check": [
            "@test",
            "@quality"
        ],
        "style": "php-cs-fixer fix --dry-run --diff",
        "style-fix": "php-cs-fixer fix",
        "quality": "phpstan analyse -c phpstan.neon",
        "quality-ci": "phpstan analyse -c phpstan.neon --error-format gitlab > quality-report.json",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-text",
        "test-coverage-ci": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit --coverage-text --colors=never --coverage-clover phpunit_clover.xml --log-junit phpunit_report.xml"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*",
            "docker": true
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.92",
        "phpstan/phpstan-symfony": "^0.12.21",
        "phpunit/phpunit": "^9",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "^1.0"
    }
}
